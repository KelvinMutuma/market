<h1>Listing instruments</h1>

<table>
  <thead>
    <tr>
      <th>Securityid</th>
      <th>Issuedate</th>
      <th>Isin</th>
      <th>Quantity issued</th>
      <th>Par value</th>
      <th>Issued price</th>
      <th>Last traded price</th>
      <th>Wtd hi price</th>
      <th>Mtd hi price</th>
      <th>Ytd hi price</th>
      <th>P12 hi price</th>
      <th>All hi price</th>
      <th>All lo price</th>
      <th>Wtd lo price</th>
      <th>Mtd lo price</th>
      <th>Ytd lo price</th>
      <th>P12 lo price</th>
      <th>Tdy share vol</th>
      <th>Wtd share vol</th>
      <th>Mtd share vol</th>
      <th>Ytd share vol</th>
      <th>P12 share vol</th>
      <th>Tdy trade vol</th>
      <th>Wtd trade vol</th>
      <th>Mtd trade vol</th>
      <th>Ytd trade vol</th>
      <th>P12 trade vol</th>
      <th>Tdy turnover</th>
      <th>Wtd turnover</th>
      <th>Mtd turnover</th>
      <th>Ytd turnover</th>
      <th>P12 turnover</th>
      <th>Previous close</th>
      <th>Foreign holdings</th>
      <th>Xc</th>
      <th>Xd</th>
      <th>Xr</th>
      <th>Foreign percentage</th>
      <th>Date</th>
      <th>Hi trade</th>
      <th>Lo trade</th>
      <th>Closing price</th>
      <th>Sendingtime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instruments.each do |instrument| %>
      <tr>
        <td><%= instrument.SecurityID %></td>
        <td><%= instrument.IssueDate %></td>
        <td><%= instrument.ISIN %></td>
        <td><%= instrument.QUANTITY_ISSUED %></td>
        <td><%= instrument.PAR_VALUE %></td>
        <td><%= instrument.ISSUED_PRICE %></td>
        <td><%= instrument.LAST_TRADED_PRICE %></td>
        <td><%= instrument.WTD_HI_PRICE %></td>
        <td><%= instrument.MTD_HI_PRICE %></td>
        <td><%= instrument.YTD_HI_PRICE %></td>
        <td><%= instrument.P12_HI_PRICE %></td>
        <td><%= instrument.ALL_HI_PRICE %></td>
        <td><%= instrument.ALL_LO_PRICE %></td>
        <td><%= instrument.WTD_LO_PRICE %></td>
        <td><%= instrument.MTD_LO_PRICE %></td>
        <td><%= instrument.YTD_LO_PRICE %></td>
        <td><%= instrument.P12_LO_PRICE %></td>
        <td><%= instrument.TDY_SHARE_VOL %></td>
        <td><%= instrument.WTD_SHARE_VOL %></td>
        <td><%= instrument.MTD_SHARE_VOL %></td>
        <td><%= instrument.YTD_SHARE_VOL %></td>
        <td><%= instrument.P12_SHARE_VOL %></td>
        <td><%= instrument.TDY_TRADE_VOL %></td>
        <td><%= instrument.WTD_TRADE_VOL %></td>
        <td><%= instrument.MTD_TRADE_VOL %></td>
        <td><%= instrument.YTD_TRADE_VOL %></td>
        <td><%= instrument.P12_TRADE_VOL %></td>
        <td><%= instrument.TDY_TURNOVER %></td>
        <td><%= instrument.WTD_TURNOVER %></td>
        <td><%= instrument.MTD_TURNOVER %></td>
        <td><%= instrument.YTD_TURNOVER %></td>
        <td><%= instrument.P12_TURNOVER %></td>
        <td><%= instrument.PREVIOUS_CLOSE %></td>
        <td><%= instrument.FOREIGN_HOLDINGS %></td>
        <td><%= instrument.XC %></td>
        <td><%= instrument.XD %></td>
        <td><%= instrument.XR %></td>
        <td><%= instrument.FOREIGN_PERCENTAGE %></td>
        <td><%= instrument.DATE %></td>
        <td><%= instrument.HI_TRADE %></td>
        <td><%= instrument.LO_TRADE %></td>
        <td><%= instrument.CLOSING_PRICE %></td>
        <td><%= instrument.SendingTime %></td>
        <td><%= link_to 'Show', instrument %></td>
        <td><%= link_to 'Edit', edit_instrument_path(instrument) %></td>
        <td><%= link_to 'Destroy', instrument, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Instrument', new_instrument_path %>
