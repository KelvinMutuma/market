<h1>Listing issuers</h1>

<table>
  <thead>
    <tr>
      <th>Issuer id</th>
      <th>Sendingtime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issuers.each do |issuer| %>
      <tr>
        <td><%= issuer.ISSUER_ID %></td>
        <td><%= issuer.SendingTime %></td>
        <td><%= link_to 'Show', issuer %></td>
        <td><%= link_to 'Edit', edit_issuer_path(issuer) %></td>
        <td><%= link_to 'Destroy', issuer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Issuer', new_issuer_path %>
